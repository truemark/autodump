    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.115.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "truemark-cdk-lib": "^1.10.0"
  },
  "engines": {
    "pnpm": "^8.0.0",
